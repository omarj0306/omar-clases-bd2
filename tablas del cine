
/**CREAMOS LA TABLA DE PELICULA

CREATE TABLE PELICULA(ID_PELICULA INTEGER,TITULO VARCHAR2(120),DIRECTOR VARCHAR2 (120),SINOPSIS VARCHAR2(255),
CONSTRAINT PK_ID_PELICULA PRIMARY KEY (ID_PELICULA)); 


/* CREAMOS LA TABLA DE HORARIO

CREATE TABLE HORARIO(ID_HORARIO INTEGER,ID_PELICULA INTEGER, FECHA DATE,
                     CONSTRAINT PK_ID_HORARIO PRIMARY KEY (ID_HORARIO),
                     CONSTRAINT FK1_ID_PELIUCLA FOREIGN KEY (ID_PELICULA)REFERENCES PELICULA (ID_PELICULA) );
                     
                     

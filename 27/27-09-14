38 package com.jc.model; 
39 
 
40 /** 
41  * 
42  * @author campitos 
43  */ 
44 
 
45 import java.sql.*; 
46 public class Conexion { 
47      
48     public static Connection conectarse()throws Exception{ 
49         String url="jdbc:oracle:thin:@localhost:1521:XE"; 
50        Class.forName("oracle.jdbc.OracleDriver"); 
51        Connection con=DriverManager.getConnection(url,"system","system"); 
52        return con; 
53     } 
54      
55 } 
56 
 
57 
 
58 TestProcedimeintos.java 
59 
 
60 package com.jc.model; 
61 
 
62 import java.sql.CallableStatement; 
63 import java.sql.Connection; 
64 
 
65 /** 
66  * 
67  * @author campitos 
68  */ 
69 public class TestProcedimiento { 
70     public static void main(String args[])throws Exception{ 
71        
72         Connection con=Conexion.conectarse(); 
73      CallableStatement callate=con.prepareCall("{call ANIOS_DIAS(?,?)}"); 
74         callate.registerOutParameter(2,java.sql.Types.INTEGER); 
75         callate.setInt(1,21); 
76          
77        
78         callate.execute(); 
79         int pk=callate.getInt(2); 
80         System.out.println("Tu edad en dias es:"+pk); 
81     } 
82      
83 } 

CREATE OR REPLACE PROCEDURE EFICIENCIA_DEVELOPER(MY_ID_TAREA OUT INTEGER, MY_ID_DEVELOPER IN INTEGER,MY_DESCRIPCION IN VARCHAR2, MY_COMPLETADA IN CHAR, MY_PORCIENTO_COMPLETADA INTEGER,MY_FECHA DATE ) 
  AS 
   BEGIN 
   SELECT SECUENCIA_TAREA.NEXTVAL INTO MY_ID_TAREA FROM DUAL; 
   INSERT INTO TAREA VALUES (MY_ID_TAREA,MY_ID_DEVELOPER, MENSAJE); 
   END; 


SELECT DEVELOPER.ID_DEVELOPER,TAREA.ID_TAREA FROM DEVELOPER;
